        <http://www.w3.org/2000/01/rdf-schema#isDefinedBy>
                <http://purl.org/ASN/schema/core/> ;
        <http://www.w3.org/2000/01/rdf-schema#label>
                "Statement Notation" ;
        <http://www.w3.org/2000/01/rdf-schema#subPropertyOf>
                <http://purl.org/dc/terms/identifer> .

<http://purl.org/ASN/schema/core/weight>
        a       <http://www.w3.org/1999/02/22-rdf-syntax-ns#Property> ;
        <http://www.w3.org/2000/01/rdf-schema#comment>
                "A textually asserted measurement of the weight, degree, percent, or strength of a recommendation, requirement, or comparison; e.g., \"15%\", \".5\"." ;
        <http://www.w3.org/2000/01/rdf-schema#domain>
                <http://purl.org/ASN/schema/core/Statement> ;
        <http://www.w3.org/2000/01/rdf-schema#isDefinedBy>
                <http://purl.org/ASN/schema/core/> ;
        <http://www.w3.org/2000/01/rdf-schema#label>
                "Weight" .

<http://purl.org/ASN/schema/core/broadAlignment>
        a       <http://www.w3.org/1999/02/22-rdf-syntax-ns#Property> ;
        <http://www.w3.org/2000/01/rdf-schema#comment>
                "The target competency covers all of the relevant concepts in the competency being described as well as relevant concepts not found in the competency being described." ;
        <http://www.w3.org/2000/01/rdf-schema#domain>
                <http://purl.org/ASN/schema/core/Statement> ;
        <http://www.w3.org/2000/01/rdf-schema#isDefinedBy>
                <http://purl.org/ASN/schema/core/> ;
        <http://www.w3.org/2000/01/rdf-schema#label>
                "Broad Alignment" ;
