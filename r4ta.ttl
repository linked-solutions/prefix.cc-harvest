<http://ns.inria.fr/ratio4ta/v1#InferredAssertion>
        a       <http://www.w3.org/2000/01/rdf-schema#Class> ;
        <http://www.w3.org/2000/01/rdf-schema#comment>
                "Represents an asserted triple that is inferred from the other triples."@en ;
        <http://www.w3.org/2000/01/rdf-schema#label>
                "Inferred Assertion" ;
        <http://www.w3.org/2000/01/rdf-schema#subClassOf>
                <http://ns.inria.fr/ratio4ta/v1#Assertion> .

<http://www.w3.org/2004/03/trix/rdfg-1/Graph>
        a       <http://www.w3.org/2000/01/rdf-schema#Class> ;
        <http://www.w3.org/2000/01/rdf-schema#comment>
                "\n        An RDF graph (with intensional semantics).\n      " ;
        <http://www.w3.org/2000/01/rdf-schema#label>
                "Graph" .

<http://ns.inria.fr/ratio4ta/v1#Assertion>
        a       <http://www.w3.org/2000/01/rdf-schema#Class> ;
        <http://www.w3.org/2000/01/rdf-schema#comment>
                "Represents an asserted triple."@en ;
        <http://www.w3.org/2000/01/rdf-schema#label>
                "Assertion" ;
        <http://www.w3.org/2000/01/rdf-schema#subClassOf>
                <http://www.w3.org/2004/03/trix/rdfg-1/Graph> .

<http://ns.inria.fr/ratio4ta/v1#antecedent>
        a       <http://www.w3.org/2002/07/owl#ObjectProperty> ;
        <http://www.w3.org/2000/01/rdf-schema#comment>
                "A property that links a justification to the justifications for the \nassertions of the triples from which the inferred tripl of the linking \njustification has been derived."@en ;
        <http://www.w3.org/2000/01/rdf-schema#domain>
                <http://ns.inria.fr/ratio4ta/v1#Justification> ;
        <http://www.w3.org/2000/01/rdf-schema#label>
                "antecedent" ;
        <http://www.w3.org/2000/01/rdf-schema#range>
                <http://ns.inria.fr/ratio4ta/v1#Justification> .

<http://ns.inria.fr/ratio4ta/v1#DirectAssertion>
        a       <http://www.w3.org/2000/01/rdf-schema#Class> ;
        <http://www.w3.org/2000/01/rdf-schema#comment>
                "Represents a directly asserted triple that represents a ground facts."@en ;
        <http://www.w3.org/2000/01/rdf-schema#label>
                "Direct Assertion" ;
        <http://www.w3.org/2000/01/rdf-schema#subClassOf>
                <http://ns.inria.fr/ratio4ta/v1#Assertion> .

<http://ns.inria.fr/ratio4ta/v1#justifies>
        a       <http://www.w3.org/2002/07/owl#ObjectProperty> ;
        <http://www.w3.org/2000/01/rdf-schema#comment>
                "A property that expresses the relation that a justification justifies the assertion of a triple."@en ;
        <http://www.w3.org/2000/01/rdf-schema#domain>
                <http://ns.inria.fr/ratio4ta/v1#Justification> ;
        <http://www.w3.org/2000/01/rdf-schema#label>
                "justifies" ;
        <http://www.w3.org/2000/01/rdf-schema#range>
                <http://ns.inria.fr/ratio4ta/v1#Assertion> .

<http://ns.inria.fr/ratio4ta/v1#inferredByRule>
        a       <http://www.w3.org/2002/07/owl#ObjectProperty> ;
        <http://www.w3.org/2000/01/rdf-schema#comment>
                "A property that relates an inferred assertion with a rule that has been enforced to infer the triple."@en ;
        <http://www.w3.org/2000/01/rdf-schema#domain>
                <http://ns.inria.fr/ratio4ta/v1#InferredAssertion> ;
        <http://www.w3.org/2000/01/rdf-schema#label>
                "inferred by rule" ;
        <http://www.w3.org/2000/01/rdf-schema#range>
                <http://ns.inria.fr/ratio4ta/v1#InferenceRule> .

<http://ns.inria.fr/ratio4ta/v1>
        a       <http://www.w3.org/2002/07/owl#Ontology> ;
        <http://www.w3.org/2000/01/rdf-schema#comment>
                "Ratio4TA (interlinked justifications for triple assertions) is a lightweight vocabulary for encoding justifications using named graphs."@en .

<http://ns.inria.fr/ratio4ta/v1#Justification>
        a       <http://www.w3.org/2000/01/rdf-schema#Class> ;
        <http://www.w3.org/2000/01/rdf-schema#comment>
                "Represents a justification."@en ;
        <http://www.w3.org/2000/01/rdf-schema#label>
                "Justification" ;
        <http://www.w3.org/2000/01/rdf-schema#subClassOf>
                <http://www.w3.org/2004/03/trix/rdfg-1/Graph> .

<http://ns.inria.fr/ratio4ta/v1#InferenceRule>
        a       <http://www.w3.org/2000/01/rdf-schema#Class> ;
        <http://www.w3.org/2000/01/rdf-schema#comment>
                "Represents an inference rule that has been enforced to infer a triple. How rules will be encoded are not restricted to a particular encoding on purpose to accommodate different kinds of rule based systems distributed across the Web."@en ;
        <http://www.w3.org/2000/01/rdf-schema#label>
                "Inference Rule" .
