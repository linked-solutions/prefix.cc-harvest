        a       <http://www.w3.org/2002/07/owl#ObjectProperty> ;
        <http://www.w3.org/2000/01/rdf-schema#comment>
                "This property relates roles and their requisites, i.e. other roles."@en ;
        <http://www.w3.org/2000/01/rdf-schema#domain>
                <http://purl.org/wai#Role> ;
        <http://www.w3.org/2000/01/rdf-schema#label>
                "requires"@en ;
        <http://www.w3.org/2000/01/rdf-schema#range>
                <http://purl.org/wai#Role> .

<http://purl.org/wai#specializes>
        a       <http://www.w3.org/2002/07/owl#TransitiveProperty> , <http://www.w3.org/2002/07/owl#ObjectProperty> ;
        <http://www.w3.org/2000/01/rdf-schema#comment>
                "This property allows to express hierarchies of roles, through of role specialization: if role A specializes role B, and B specializes C, then role A specializes role C."@en ;
        <http://www.w3.org/2000/01/rdf-schema#domain>
                <http://purl.org/wai#Role> ;
        <http://www.w3.org/2000/01/rdf-schema#label>
                "specializes"@en ;
