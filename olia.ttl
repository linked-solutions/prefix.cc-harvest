<http://purl.org/olia/olia.owl#MannerAdverb>
        a       <http://www.w3.org/2002/07/owl#Class> ;
        <http://www.w3.org/2000/01/rdf-schema#subClassOf>
                <http://purl.org/olia/olia.owl#Adverb> ;
        <http://www.w3.org/2002/07/owl#deprecated>
                true ;
