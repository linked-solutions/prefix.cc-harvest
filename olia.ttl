        <http://www.w3.org/2002/07/owl#versionInfo>
                "http://purl.org/olia/mte/multext-east.owl#ElementWithoutClitic\"" .

<http://purl.org/olia/olia.owl#NounHeadedPhrase>
        a       <http://www.w3.org/2002/07/owl#Class> ;
        <http://www.w3.org/2000/01/rdf-schema#comment>
                "\n\t\t\tA NounHeadedPhrase takes a nominal as its (semantic) head. \n\t\t\tIntroduced as a generalization over NounPhrase and PrepositionalPhrase for reasons of consistency with dependency parsers like \n\t\t\tConnexor where this differentiation is not made.\n\t\t" ;
        <http://www.w3.org/2000/01/rdf-schema#subClassOf>
                <http://purl.org/olia/olia.owl#Phrase> .

