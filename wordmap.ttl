<http://purl.org/net/ns/wordmap#example2> <http://purl.org/net/ns/ex#code> "\n<rdf:Description rdf:about=\"http://example.org/theater_page\">\n <wm:keywordSet>\n  <wm:WordSet>\n   <wm:associatedWord rdf:resource=\"http://www.kanzaki.com/ns/keyword/opera#word\"/>\n   <wm:associatedWord rdf:resource=\"http://www.kanzaki.com/ns/keyword/ballet#word\"/>\n  </wm:WordSet>\n </wm:keywordSet>\n</rdf:Description>\n\n<wm:Word rdf:about=\"http://www.kanzaki.com/ns/keyword/opera#word\">\n <wm:primarySense>\n  <wm:Subject rdf:about=\"http://example.org/psi#Opera_music\">\n   <wm:category rdf:resource=\"http://example.org/category/Theatrical_Music\"/>\n  </wm:Subject>\n </wm:primarySense>\n</wm:Word>\n\n<wm:Word rdf:about=\"http://www.kanzaki.com/ns/keyword/ballet#word\">\n <wm:primarySense>\n  <wm:Subject rdf:about=\"http://example.org/psi#Ballet\">\n   <wm:category rdf:resource=\"http://example.org/category/Theatrical_Music\"/>\n  </wm:Subject>\n </wm:primarySense>\n</wm:Word>\n " .
