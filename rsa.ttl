                <http://www.w3.org/ns/auth/cert#int> ;
        <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status>
                "unstable" .

<http://www.w3.org/ns/auth/rsa#RSAKey>
        a       <http://www.w3.org/2002/07/owl#Class> ;
        <http://www.w3.org/2000/01/rdf-schema#comment>
                "\n    The union of the public and private components of an RSAKey.\n    Usually those pieces are not kept together\n    "@en ;
        <http://www.w3.org/2000/01/rdf-schema#label>
                "RSA Key"@en ;
        <http://www.w3.org/2000/01/rdf-schema#subClassOf>
                <http://www.w3.org/ns/auth/cert#Key> ;
        <http://www.w3.org/2003/06/sw-vocab-status/ns#term_status>
